    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "20.11.1",
    "yarn": "4.1.1"
  }
